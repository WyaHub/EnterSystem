-- Scripts:

local function VOZIAI_fake_script() -- RGBButtonBar.Rainbow Effect  
	local script = Instance.new('Script', RGBButtonBar)

	function zigzag(X) return math.acos(math.cos(X*math.pi))/math.pi end

	counter = 0

	while wait(0.1)do
		script.Parent.BackgroundColor3 = Color3.fromHSV(zigzag(counter),1,1)

		counter = counter + 0.01
	end


end
coroutine.wrap(VOZIAI_fake_script)()
local function OAVJZKL_fake_script() -- Enter.Animate 
	local script = Instance.new('LocalScript', Enter)

	local noob = script.Parent


	Tween = function(Obj, Time, Settings)	
		game:GetService("TweenService"):Create(Obj, TweenInfo.new(Time), Settings):Play()
	end


	script.Parent.MouseEnter:Connect(function()
		Tween(noob, 0.5, {BackgroundTransparency = 0.6})
	end)

	script.Parent.MouseLeave:Connect(function()
		Tween(noob, 0.5, {BackgroundTransparency = 0.8})
	end)
end
coroutine.wrap(OAVJZKL_fake_script)()
local function AZTWP_fake_script() -- Enter.ClickForExploit 
	local script = Instance.new('Script', Enter)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Visible = false 
	end)

end
coroutine.wrap(AZTWP_fake_script)()
